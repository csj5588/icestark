{
  "name": "@icedesign/stark-layout-scaffold",
  "version": "0.1.0",
  "description": "微前端方案 icestark 的框架应用，用于快速初始化框架应用",
  "dependencies": {
    "@alifd/next": "^1.x",
    "@alifd/theme-design-pro": "^0.x",
    "@ice/stark-data": "^0.1.2",
    "@icedesign/container": "^1.x",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "antd": "^3.12.1",
    "axios-service": "^1.4.4",
    "classnames": "^2.2.5",
    "concurrently": "^4.1.2",
    "ik-utils": "git+ssh://git@code.inke.cn:opd/fe-aws/modules/ik-utils.git",
    "moment": "^2.24.0",
    "prop-types": "^15.5.8",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^5.0.7",
    "react-router-redux": "^4.0.8"
  },
  "devDependencies": {
    "@ice/spec": "^1.0.1",
    "build-plugin-fusion": "^0.1.0",
    "build-plugin-icestark": "^1.0.2",
    "build-plugin-moment-locales": "^0.1.0",
    "chalk": "^4.1.0",
    "eslint": "^6.0.1",
    "ice.js": "^1.0.0",
    "stylelint": "^13.1.0"
  },
  "scripts": {
    "start": "concurrently \"npm run start-react\" \"npm run start-vue\" \"npm run start-tark\" ",
    "start-tark": "icejs start",
    "start-react": "cd ./apps/react-icestark-child-16/ && npm run start",
    "start-vue": "cd ./apps/vue-icestark-child/ && npm run start",
    "i-all": "npm install chalk && cross-env-shell node scripts/install.js",
    "i-all:cnpm": "cnpm install chalk && cross-env-shell INSTALL_SCRIPT=cnpm node scripts/install.js",
    "build": "icejs build",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx ./",
    "stylelint": "stylelint ./**/*.scss"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alibaba-fusion/materials/tree/master/scaffolds/ice-stark-layout"
  },
  "iceworks": {
    "type": "react",
    "adapter": "adapter-react-v3"
  },
  "ideMode": {
    "name": "ice-react"
  },
  "private": true,
  "originTemplate": "@icedesign/stark-layout-scaffold"
}
