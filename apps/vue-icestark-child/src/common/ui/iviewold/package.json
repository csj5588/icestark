    "style-loader": "^0.13.1",
    "url-loader": "^0.5.9",
    "vue": "^2.4.1",
    "vue-hot-reload-api": "^1.3.3",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^11.3.4",
    "vue-router": "^2.7.0",
