    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lolex": "^1.5.2",
    "mocha": "^3.4.2",
    "sinon": "^1.17.7",
