{
  "plugins": [
    "build-plugin-icestark",
    [
      "build-plugin-fusion",
      {
        "themePackage": "@alifd/theme-design-pro",
        "themeConfig": {
          "nextPrefix": "next-icestark-"
        }
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ]
  ],
  "proxy": {
    "/api/v1/**": {
      "enable": true,
      "target": "http://testapi.imilive.cn/",
      "name": ""
    },
    "/upload/**": {
      "enable": true,
      "target": "https://upload.inke.cn/",
      "name": "资源上传接口（禁止修改）"
    },
    "/login/**": {
      "enable": true,
      "target": "https://testqarbac.busi.inke.cn/",
      "name": "登录相关接口（禁止修改）"
    },
    "/web/op_log/**": {
      "enable": true,
      "target": "https://testrbac.busi.inke.cn/",
      "name": "日志管理接口（禁止修改）"
    },
    "/api/log/analytics/**": {
      "enable": true,
      "target": "https://betaservice.busi.inke.cn/",
      "name": "埋点上报接口（禁止修改）"
    },
    "/api_web/v1/**": {
      "enable": true,
      "target": "http://testapi.meeshow.com/",
      "name": "serviceInfo"
    }
  }
}
